<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
	<Appenders>
		<Console name="console" target="SYSTEM_OUT">
			<PatternLayout/>
		</Console>
	</Appenders>
	
	<!-- Fatal, Error, Warn, Info, Debug, trace -->
	<Loggers>
		<Root level="info">
			<AppenderRef ref="console"/>
		</Root>
	</Loggers>
</Configuration>